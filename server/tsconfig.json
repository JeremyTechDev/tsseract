{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../dist/server",
    "rootDir": "./",
    "sourceMap": false,
    "strict": true,
    "removeComments": true,
    "noEmitOnError": false,
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmit": false
  }
}
