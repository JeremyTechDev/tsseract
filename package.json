{
  "name": "tsseract",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next ./dist/client",
    "server": "ts-node-dev --respawn --no-notify  ./dist/server/index",
    "dev:server": "ts-node-dev --respawn --no-notify ./dist/server/index & next ./dist/client",
    "tsc": "tsc -p ./server & tsc -p ./client",
    "tsc:watch": "tsc -p ./server -w & tsc -p ./client -w",
    "start": "next start",
    "build": "next build",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@zeit/next-sass": "^1.0.1",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "joi-password-complexity": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16",
    "next": "^9.4.4",
    "node-sass": "^4.14.1",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.4",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/material-ui": "^0.21.8",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^14.0.22",
    "@types/node-sass": "^4.11.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.43",
    "@types/supertest": "^2.0.10",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.1.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.5.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.51",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.js --fix-dry-run"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
